<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".AnotherActivity">
    <EditText
        android:id="@+id/editTextGet"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="Enter data"/>

    <Button
        android:id="@+id/buttonGetCacheInternal"
        android:onClick="getCacheInternal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="CACHE INTERNAL"/>

    <Button
        android:id="@+id/buttonGetCacheExternal"
        android:onClick="getCacheExternal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Cache External"/>

    <Button
        android:id="@+id/buttonGetFileStoragePrivate"
        android:onClick="getFileStoragePrivate"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="File Storage Private"/>

    <Button
        android:id="@+id/buttonGetFileStoragePublic"
        android:onClick="getFileStoragePublic"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="File Storage Public"/>

</LinearLayout>